
@prefix dt: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix person: <http://people.csail.mit.edu/stewart/person.n3#> .
@prefix nl: <http://people.csail.mit.edu/stewart/nl.n3#> .

@prefix : <#> .  # this document


<#Contact>	a owl:Class;
	rdfs:comment "";
	rdfs:label "Contact" .


<#person>	a owl:Property;
	rdfs:label "Person";
	nl:prep "with";
	rdfs:range person:Person .

<#email>	a owl:Property;
	rdfs:label "Location";
	nl:prep "email";
	nl:prep "email is";
	nl:prep "mbox";
	nl:prep "at";
	rdfs:range person:Email .