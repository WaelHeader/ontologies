Ontology(<http://onto.ir/ontologies/2011/wsn>
Annotation(rdfs:comment "TODO: Add Unit class of http://www.qudt.org/ ontology as an equvalent class to MeasureUnit"^^xsd:string)
Annotation(rdfs:label "Wireless Sensor Network Ontology"@en)
Annotation(owl:versionInfo "1.0.0"^^xsd:string)

Declaration(Class(wsn:BatteryComponent))
SubClassOf(wsn:BatteryComponent wsn:ProcessingComponent)
Declaration(Class(wsn:Component))
Declaration(Class(wsn:Connection))
Declaration(Class(wsn:MeasureUnit))
Declaration(Class(wsn:ProcessingComponent))
SubClassOf(wsn:ProcessingComponent wsn:Component)
Declaration(Class(wsn:RadioComponent))
SubClassOf(wsn:RadioComponent wsn:ProcessingComponent)
Declaration(Class(wsn:Revision))
Declaration(Class(wsn:SensorComponent))
SubClassOf(wsn:SensorComponent wsn:Component)
Declaration(Class(wsn:SensorNode))
Declaration(ObjectProperty(wsn:hasComponent))
ObjectPropertyRange(wsn:hasComponent wsn:Component)
Declaration(ObjectProperty(wsn:hasDestinationNode))
ObjectPropertyDomain(wsn:hasDestinationNode wsn:Connection)
ObjectPropertyRange(wsn:hasDestinationNode wsn:SensorNode)
Declaration(ObjectProperty(wsn:hasMeasureUnit))
ObjectPropertyRange(wsn:hasMeasureUnit wsn:MeasureUnit)
Declaration(ObjectProperty(wsn:hasRevision))
ObjectPropertyRange(wsn:hasRevision wsn:Revision)
Declaration(ObjectProperty(wsn:hasSourceNode))
ObjectPropertyDomain(wsn:hasSourceNode wsn:Connection)
ObjectPropertyRange(wsn:hasSourceNode wsn:SensorNode)
)
